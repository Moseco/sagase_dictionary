import 'vocabs.dart';

CREATE INDEX IX_vocab_writings_writing_search_form
ON vocab_writings(writing_search_form) WHERE writing_search_form IS NOT NULL;

CREATE INDEX IX_vocab_readings_reading_search_form
ON vocab_readings(reading_search_form) WHERE reading_search_form IS NOT NULL;

